	"scripts": {
		"ios:device": "ns debug ios --team-id='HA4T8F6D7M' --env.env=development --for-device",
		"android:device": "ns run android --env.production --env.aot --env.compileSnapshot --env.uglify --env.snapshot --env.sourceMap --env.hiddenSourceMap --clean",
		"e2e:android:build": "android.att.debug",
		"e2e:android:build:debug": "ns clean; ns build android; detox build -c android.emu.debug",
		"e2e:android:debug:test": "detox test -c android.emu.debug",
		"e2e:ios:build:debug": "detox build -c ios.sim.debug --env.e2e",
		"e2e:ios:test": "detox test -c ios.sim.debug",
		"animations:enable": "./node_modules/.bin/enable-animations",
		"android": "ns build android --env.production --env.aot --env.compileSnapshot --env.uglify --env.snapshot --env.sourceMap --env.hiddenSourceMap --clean --release --key-store-path ./store/android.jks --key-store-password Nasrda@2025 --key-store-alias nasrda --key-store-alias-password Dev@Nasrda=2025",
		"ios": "ns build ios --env.production --env.aot --env.uglify --env.sourceMap --env.hiddenSourceMap --clean --release --for-device --provision='Nasrda Distro'",
		"android:dev": "bun android --aab --env.env=development --copy-to ./dist/android-dev.aab",
		"android:dev:apk": "bun android --env.env=development --copy-to ./dist/android-dev.apk",
		"ios:dev": "bun ios --env.env=development --copy-to ./dist/ios-dev.ipa",
		"build:dev": "bun android:dev && bun ios:dev",
		"android:stag": "bun android --aab --env.env=staging --copy-to ./dist/android-stag.aab",
		"android:stag:apk": "bun android --env.env=staging --copy-to ./dist/android-stag.apk",
		"ios:stag": "bun ios --env.env=staging --copy-to ./dist/ios-stag.ipa",
		"build:stag": "bun android:stag && bun android:stag:apk && bun ios:stag",
		"android:prod": "bun android --aab --env.env=production --copy-to ./dist/android-prod.aab",
		"android:prod:apk": "ns build android --env.env=production --copy-to ./dist/android-prod.apk",
		"ios:prod": "bun ios --env.env=production --copy-to ./dist/ios-prod.ipa",
		"build:prod": "bun android:prod && bun ios:prod"
	},
